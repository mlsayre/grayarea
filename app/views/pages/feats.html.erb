<script>
$(document).ready(function() {
	// populate medals
	var givenum = $(".giverfeats .featshow img").length
	var giveurl = "https://s3-us-west-2.amazonaws.com/wordstretch/awards/obj_creator_medal_0NUMBER.png"
	for (var i = 1; i < givenum + 1; i++) {
		var thenum = i.toString().padStart(2, "0")
		var theurl = giveurl.replace("NUMBER", thenum);
		$(".giverfeats .featshow img").eq(i-1).attr("src", theurl);
	}
	var guessnum = $(".guesserfeats .featshow img").length
	var guessurl = "https://s3-us-west-2.amazonaws.com/wordstretch/awards/obj_guesser_medal_0NUMBER.png"
	for (var i = 1; i < guessnum + 1; i++) {
		var thenum = i.toString().padStart(2, "0")
		var theurl = guessurl.replace("NUMBER", thenum);
		$(".guesserfeats .featshow img").eq(i-1).attr("src", theurl);
	}

	var giverstatus = gon.giverstatus;
	var guesserstatus = gon.guesserstatus;
	var givernotify = gon.givernotify;
	var guessernotify = gon.guessernotify;

	for (var i = 0; i < giverstatus.length; i++) {
		if (giverstatus[i] !== 0) {
			$(".givefeat" + i).removeClass("hidefeat").addClass("showfeat");
		}
	}
	for (var i = 0; i < guesserstatus.length; i++) {
		if (guesserstatus[i] !== 0) {
			$(".guessfeat" + i).removeClass("hidefeat").addClass("showfeat");
		}
	}
	for (var i = 0; i < givernotify.length; i++) {
		if (givernotify[i] !== 0) {
			$(".givefeat" + i).addClass("newfeat");
		}
	}
	for (var i = 0; i < guessernotify.length; i++) {
		if (guessernotify[i] !== 0) {
			$(".guessfeat" + i).addClass("newfeat");
		}
	}
	setTimeout(function() {
	  $.ajax({
      url: "/pages/resetstatnotify",
      type: "POST"
    })
	}, 500)
	$(document).ready(function() {
		$("body").addClass("bodyscrollable");
	})
});
</script>